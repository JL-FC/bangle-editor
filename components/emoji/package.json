{
  "name": "@jl-fc/emoji",
  "version": "0.32.2",
  "homepage": "https://bangle.dev",
  "authors": [
    {
      "name": "Kushan Joshi",
      "email": "0o3ko0@gmail.com",
      "web": "http://github.com/kepta"
    }
  ],
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "style": "style.css",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JL-FC/bangle-editor.git"
  },
  "bugs": {
    "url": "https://github.com/JL-FC/bangle-editor/issues"
  },
  "scripts": {
    "compile-ts": "yarn g:tsc --build $INIT_CWD",
    "watch-ts": "yarn g:tsc -p $INIT_CWD -w",
    "release-package": "npm publish --otp $OTP"
  },
  "dependencies": {
    "@jl-fc/utils": "workspace:*",
    "@types/jest": "^29.5.3",
    "markdown-it-emoji": "^2.0.2"
  },
  "peerDependencies": {
    "@jl-fc/core": "*",
    "@jl-fc/pm": "*"
  },
  "devDependencies": {
    "@jl-fc/all-base-components": "workspace:*",
    "@jl-fc/core": "workspace:*",
    "@jl-fc/markdown": "workspace:*",
    "@jl-fc/pm": "workspace:*",
    "@jl-fc/table": "workspace:*",
    "@jl-fc/test-helpers": "workspace:*",
    "@types/markdown-it": "^12.2.3",
    "@types/markdown-it-emoji": "^2.0.2",
    "prosemirror-markdown": "^1.11.1"
  },
  "publishConfig": {
    "access": "public",
    "main": "dist/index.cjs",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
      ".": {
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      },
      "./style.css": "./style.css"
    },
    "registry": "https://npm.pkg.github.com/"
  }
}
